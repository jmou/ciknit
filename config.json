{
  "static-steps": {
    "_db": {
      "action": "bigquery/session",
      "params": {
        "in/param/user": "inline:joe"
      }
    }
  },
  "extensions": {
    ".ipynb": {
      "action": "python/papermill",
      "pre": "pre/python/papermill",
      "params": {
        "in/param/session": "_pos:_db:out/session",
        "in/param/root/knitpyter.py": "file:knitpyter.py",
        "in/param/in.ipynb": "original:"
      }
    },
    ".lift": {
      "action": "bigquery/lift",
      "pre": "pre/bigquery/lift",
      "params": {
        "in/param/session": "_pos:_db:out/session"
      }
    },
    ".sql": {
      "action": "bigquery/transform",
      "pre": "pre/bigquery/transform",
      "params": {
        "in/param/session": "_pos:_db:out/session",
        "in/param/query": "pre:sql"
      }
    },
    ".txt": {
      "action": "python/venv",
      "params": {
        "in/param/venvcache": "inline:/var/tmp/knit/venvcache",
        "in/param/requirements.txt": "original:"
      }
    }
  }
}
